{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "identifier": "cloud.oneoh.onebox",
  "productName": "OneBox",
  "app": {
    "windows": [
      {
        "devtools": true,
        "maximizable": false,
        "decorations": true,
        "resizable": false,
        "title": "OneBox",
        "width": 320,
        "height": 600
      }
